    "advisors": [
        {
            "name": "Mustafa Yavuz",
            "picture": "Mustafa_Yavuz.jpg",
            "website": "",
            "description": "Professor Mustafa Yavuz envisions a production tool that can harness new types of magnetic templates and exploit them to make radically new devices and materials. These templates are an enabling tool for self-assembly manufacture/production of magnetic particles into new nano-structured macro-materials, with various applications the field of nanotechnolgoy."
        },
        {
            "name": "Bo Cui",
            "picture": "Bo_Cui.jpg",
            "website": "",
            "description": "Cui leads the Waterloo Nanofabrication Group whose research is focused on nano- and microstructure fabrication using Nano-Imprint Lithography (NIL) and electron beam lithography, thin film deposition and etching, with applications in biomedical, nanoelectric and other areas."
        }
